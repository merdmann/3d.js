{
  "name": "electron-boilerplate",
  "productName": "Electron Boilerplate",
  "description": "Starter for your Electron application",
  "version": "0.0.0",
  "author": "Mr. Gumby <mr.gumby@example.com>",
  "copyright": "Â© 2016, Gumby inc.",
  "homepage": "http://example.com",
  "license": "MIT",
  "main": "app/background.js",
  "build": {
    "appId": "com.example.electron-boilerplate",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "publish": null
  },
  "scripts": {
    "postinstall": "install-app-deps",
    "build": "gulp build",
    "prerelease": "gulp build --env=production",
    "release": "build",
    "start": "gulp start",
    "pretest": "gulp build-unit --env=test",
    "test": "electron-mocha app/specs.js.autogenerated --renderer --require source-map-support/register",
    "coverage": "npm test -- -R scripts/istanbul-reporter",
    "pree2e": "gulp build-e2e --env=test",
    "e2e": "mocha app/e2e.js.autogenerated --require source-map-support/register",
    "debug": "./node_modules/.bin/electron app/background.js --debug-brk=5858"
  },
  "dependencies": {
    "ajv": "^6.5.3",
    "eslint-config-defaults": "^9.0.0",
    "eslint-plugin-import": "^2.2.0",
    "fs-jetpack": "^0.12.0",
    "gulp-cli": "^1.4.0",
    "install": "^0.12.1",
    "jquery": "^3.2.1",
    "npm": "^6.4.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "chai": "^3.5.0",
    "chai-as-promised": "^7.1.1",
    "electron": "1.6.2",
    "electron-builder": "^14.5.2",
    "electron-mocha": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-config-defaults": "^9.0.0",
    "eslint-config-google": "^0.7.1",
    "eslint-config-walmart": "^1.2.2",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-import": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-batch": "^1.0.5",
    "gulp-less": "^3.5.0",
    "gulp-plumber": "^1.2.0",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^4.3.11",
    "istanbul": "^0.4.5",
    "minimist": "^1.2.0",
    "mocha": "^3.2.0",
    "rollup": "^0.41.6",
    "rollup-plugin-istanbul": "^1.1.0",
    "source-map-support": "^0.4.11",
    "spectron": "^3.8.0",
    "three": "^0.84.0"
  }
}
